<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
 	<class name="com.mike.server.database.tables.POS_IN_MENU" table="POS_IN_MENU">
     	<meta attribute="class-description">
         	This class contains the employee detail. 
      	</meta>
      	<composite-id>
      	<key-many-to-one name="id" class="com.mike.server.database.tables.ITEMS">
      		<column name="id" />
      	</key-many-to-one>
      	      	<key-many-to-one name="location" class="com.mike.server.database.tables.MENU_PAGE">
      		<column name="name" />
      	</key-many-to-one>
      	</composite-id>


   </class>
</hibernate-mapping>